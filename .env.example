# LangSmith Tracing Configuration (Optional)
# Enable tracing to monitor LangGraph workflow execution
# Grab an API key, and set to true to enable langsmith tracing
LANGSMITH_TRACING=false
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_PROJECT=pairreader

# Anthropic API Key (Required)
# Get your API key from: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tavily API Key (Optional)
# Currently not used in the application
TAVILY_API_KEY=your_tavily_api_key_here

# Chainlit Authentication Secret (Required)
# Generate with: chainlit create-secret
CHAINLIT_AUTH_SECRET=your_chainlit_secret_here
